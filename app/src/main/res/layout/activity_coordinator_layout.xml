<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.aserbao.aserbaoanimationsummart.classify.module.ui.coordinatorLayout.CoordinatorLayoutActivity">
    <Button
        android:onClick="btn_simple"
        android:text="simple"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button16"/>
    <Button
        android:textAllCaps="false"
        android:onClick="btn_simple_custom"
        android:text="simple_custom_behavior"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button16"
        android:layout_toRightOf="@+id/button16"
        android:layout_toEndOf="@+id/button16"/>
</RelativeLayout>
