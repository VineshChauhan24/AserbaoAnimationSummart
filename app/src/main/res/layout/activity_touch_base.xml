<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.aserbao.aserbaoanimationsummart.activitys.TouchBaseActivity">
    <Button
        android:id="@+id/btn_gesture"
        android:text="手势测试"
        android:onClick="btn_gesture"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/btn_dispatch"
        android:onClick="btn_dispatch"
        android:text="事件分发"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/btn_gesture"
        android:layout_toEndOf="@+id/btn_gesture"/>
</RelativeLayout>
