<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.aserbao.aserbaoanimationsummart.classify.module.ui.UIActivity">
    <Button
        android:onClick="btn_recycler"
        android:textAllCaps="false"
        android:text="recycler_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button15"/>
    <Button
        android:onClick="btn_coordinatorLayout"
        android:textAllCaps="false"
        android:text="CoordinatorLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/button15"
        android:layout_toEndOf="@+id/button15"
        android:id="@+id/button17"/>
    <Button
        android:onClick="btn_scroll_view"
        android:textAllCaps="false"
        android:text="ScrollView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/button17"
        android:layout_toEndOf="@+id/button17"
        android:id="@+id/button18"/>
    <Button
        android:onClick="btn_time_picker"
        android:textAllCaps="false"
        android:text="timepicker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button15"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/button21"/>
    <Button
        android:onClick="btn_smart_refresh_layout"
        android:textAllCaps="false"
        android:text="smart_refresh_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button15"
        android:layout_toLeftOf="@+id/button18"
        android:layout_toStartOf="@+id/button18"
        android:id="@+id/button20"/>
    <Button
        android:onClick="btn_progress_bar"
        android:text="progressBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button21"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"/>
    <Button
        android:onClick="btn_fragment"
        android:text="fragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button18"
        android:layout_toRightOf="@+id/button20"
        android:layout_toEndOf="@+id/button20"/>
</RelativeLayout>
